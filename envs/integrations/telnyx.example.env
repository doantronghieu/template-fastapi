# TELNYX Integration Configuration

# Telnyx API Key (from portal.telnyx.com)
TELNYX_API_KEY=your_api_key_here

# Telnyx API Secret
TELNYX_API_SECRET=your_api_secret_here

# Telnyx SIP Trunk ID for LiveKit integration
TELNYX_SIP_TRUNK_ID=your_sip_trunk_id_here

# Telnyx Phone Number (E.164 format, e.g., +15551234567)
TELNYX_PHONE_NUMBER=+15551234567

# Webhook Secret for verifying Telnyx callbacks
TELNYX_WEBHOOK_SECRET=your_webhook_secret_here
